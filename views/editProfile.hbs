<!-- register -->
	<div class="register">
		<div class="container">
			<h2>Edit your profile</h2>
			 <div class="login-form-grids">
				<label class="err">{{usedMail}}</label>
				{{#each result}}
				<form id="form" action="/users/editProfileSubmit?id={{this._id}}" method="POST">
					<input type="text" name="fname" value="{{this.fname}}" ><br>
					<input type="text" name="lname" value="{{this.lname}}" ><br>
					<input type="text" name="occupation" value="{{this.occupation}}" ><br>
					<input type="text" name="country" value="{{this.country}}" ><br>
					<input type="text" name="mobile" value="{{this.mobile}}"><br>
					<input type="text" name="mail"  value="{{this.mail}}"><br>
					<input type="submit" value="Update">
				</form>
				{{/each}}
			</div>
		</div>
	</div>
