					<input type="text" con="mobile" placeholder="Mobile number..."><br>
					<input type="text" name="mail"  placeholder="Email Address"><br>
					<input type="password" id="pass" name="pass" onchange="validatePassword()" placeholder="Password"><br>
					<input type="password" id="confirm" name="confirm" onkeyup="validatePassword()" placeholder="Confirm Password">
					<label style="color: rgb(254, 145, 38);" id="validate_confirm"></label>
					<input type="hidden" name="status" value="Block">
					<input type="hidden" name="coupon" value="{{coname}}">
					<input type="hidden" name="referral" id="referral">
					<input type="hidden" name="suggested" value="{{suggested}}" >
					<input type="submit" onclick="generate()" value="Register">
				</form>
			</div> 
			<div class="register-home">
				<a href="/">Home</a>
			</div>
		</div>
	</div>
<script>
	function generate() {
            let num = '1234567890'
            let referral = ''
            for (let i = 0; i < 6; i++) {
                referral += num[Math.floor(Math.random() * 10)];/* random method 0 to 1 */
            }
            document.getElementById('referral').value=referral
    }

</script>	
<script>
function validatePassword() {
	
var password = document.getElementById("pass")
var confirm_password = document.getElementById("confirm");

  if(password.value != confirm_password.value) {
    document.getElementById("validate_confirm").innerHTML="Please Confirm Your Password"
  } else {
    document.getElementById("validate_confirm").innerHTML=" "
  }
}

</script>
